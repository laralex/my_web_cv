{
  "name": "create-wasm-app",
  "version": "0.1.0",
  "description": "create an app to consume rust-generated wasm packages",
  "main": "index.js",
  "bin": {
    "create-wasm-app": ".bin/create-wasm-app.js"
  },
  "config": {
    "devport": "5000"
  },
  "scripts": {
    "build": "export SET NODE_OPTIONS=--openssl-legacy-provider && webpack --config webpack.prod.cjs",
    "start-dev": "export SET NODE_OPTIONS=--openssl-legacy-provider && webpack-dev-server --port ${DEVPORT:=8080}",
    "start": "export SET NODE_OPTIONS=--openssl-legacy-provider && webpack serve --open --config webpack.dev.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laralex/my_web_cv.git"
  },
  "keywords": [
    "webassembly",
    "wasm",
    "rust",
    "webpack"
  ],
  "author": "Alexey Larionov <alxs.larionov@gmail.com>",
  "license": "CC0-1.0",
  "homepage": "https://github.com/laralex/my_web_cv",
  "dependencies": {
    "my-wasm": "file:./wasm"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^5.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.10.0"
  }
}
